    "@nestjs/axios": "3.0.1",
    "@nestjs/common": "^10.2.8",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.8",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mongoose": "10.0.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.8",
    "@ngrx/effects": "^16.3.0",
